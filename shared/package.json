{
  "name": "@skill-sharing/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/shared.cjs",
  "module": "dist/shared.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./domain": {
      "types": "./dist/domain.d.ts",
      "import": "./dist/domain.js",
      "require": "./dist/domain.cjs"
    },
    "./util": {
      "types": "./dist/util.d.ts",
      "import": "./dist/util.js",
      "require": "./dist/util.cjs"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "distclean": "rm -rf node_modules"
  },
  "dependencies": {
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1"
  },
  "devDependencies": {
    "@swc/core": "1.11.24",
    "typescript": "5.8.3",
    "unplugin-swc": "1.5.2",
    "vite": "6.3.3",
    "vite-plugin-dts": "4.5.3",
    "vitest": "3.1.2"
  }
}
